#!/bin/sh
#    _____ _     ____  ____  ____  ____  _____
#   /    // \   /  _ \/  __\/  _ \/  __\/__ __\
#   |  __\| |   | / \||  \/|| / \||  \/|  / \
#   | |   | |_/\| |-|||  __/| \_/||    /  | |
#   \_/   \____/\_/ \|\_/   \____/\_/\_\  \_/
#

alpha="$(alpha get)"
[ "$alpha" -lt 95 ] && alpha set 99
flameshot gui
[ "$alpha" -lt 95 ] && alpha set "$alpha"

